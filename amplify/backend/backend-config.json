{
    "storage": {
        "currencytable": {
            "service": "DynamoDB",
            "providerPlugin": "awscloudformation"
        }
    },
    "function": {
        "currencyfunction": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true,
            "dependsOn": [
                {
                    "category": "storage",
                    "resourceName": "currencytable",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                }
            ]
        },
        "testfunc": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true
        }
    },
    "api": {
        "gqllambdacrypto": {
            "service": "AppSync",
            "providerPlugin": "awscloudformation",
            "output": {
                "securityType": "API_KEY"
            }
        }
    }
}